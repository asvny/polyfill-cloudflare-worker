{
  "extends": "eslint:recommended",
  "env": {
    "es6": false,
    "browser": true,
    "node": false
  },
  "parserOptions": {
    "ecmaVersion": 3,
    "sourceType": "script"
  },
  "rules": {
    "comma-dangle": ["error", "never"],
    "quote-props": ["error", "as-needed", { "keywords": true }],
    "dot-notation": ["error", { "allowKeywords": false }],
    "radix": "error",
    "no-catch-shadow": "error",
    "no-dupe-else-if": "error"
  },
  "root": true
}
